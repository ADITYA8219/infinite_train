@model IEnumerable<NorthwindMVC.Models.Customer>

<h2>German Customers</h2>
<table>
    <tr>
        <th>Customer ID</th>
        <th>Company Name</th>
        <th>City</th>
    </tr>
    @foreach (var customer in Model)
    {
<tr>
    <td>@customer.CustomerID</td>
    <td>@customer.CompanyName</td>
    <td>@customer.City</td>
</tr>}
</table>
